<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mvp.view.mine.TagActivity">

    <com.hjq.bar.TitleBar
        android:id="@+id/createTag_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="@string/tag_mytag" />

    <ImageView
        android:id="@+id/tag_imageView3"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/tag_pic3"
        app:layout_constraintBottom_toTopOf="@+id/tag_imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView2"
        app:layout_constraintTop_toBottomOf="@+id/tag_textView_top" />

    <ImageView
        android:id="@+id/tag_imageView2"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/tag_pic2"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView3"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView1"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView3" />

    <ImageView
        android:id="@+id/tag_imageView1"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/tag_pic1"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView2"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView2" />

    <ImageView
        android:id="@+id/tag_imageView"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="24dp"
        android:background="@drawable/tag_pic0"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView1"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView1" />

    <ImageView
        android:id="@+id/tag_imageView7"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/tag_pic7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView6"
        app:layout_constraintTop_toBottomOf="@+id/tag_imageView3" />

    <ImageView
        android:id="@+id/tag_imageView5"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/tag_pic5"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView6"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView4"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView6" />

    <ImageView
        android:id="@+id/tag_imageView4"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="24dp"
        android:background="@drawable/tag_pic4"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView5"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView5" />

    <ImageView
        android:id="@+id/tag_imageView6"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/tag_pic6"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView7"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView5"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView7" />

    <ImageView
        android:id="@+id/tag_imageView11"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/tag_pic11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView10"
        app:layout_constraintTop_toBottomOf="@+id/tag_imageView7" />

    <ImageView
        android:id="@+id/tag_imageView9"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/tag_pic9"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView10"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView10"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView8"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView10" />

    <ImageView
        android:id="@+id/tag_imageView10"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/tag_pic10"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView11"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tag_imageView9"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView11" />

    <ImageView
        android:id="@+id/tag_imageView8"
        android:layout_width="73dp"
        android:layout_height="85dp"
        android:layout_marginStart="24dp"
        android:background="@drawable/tag_pic8"
        app:layout_constraintBottom_toBottomOf="@+id/tag_imageView9"
        app:layout_constraintEnd_toStartOf="@+id/tag_imageView9"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tag_imageView9" />

    <TextView
        android:id="@+id/tag_textView_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/tag_choicemytag"
        android:textColor="#666666"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/createTag_title" />

    <TextView
        android:id="@+id/tag_textView_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="24dp"
        android:text="@string/tag_leastOne"
        app:layout_constraintBottom_toTopOf="@+id/tag_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tag_imageView11" />

    <Button
        android:id="@+id/tag_button"
        android:layout_width="177dp"
        android:layout_height="38dp"
        android:layout_marginBottom="187dp"
        android:background="@mipmap/bg_send"
        android:text="@string/tag_create_tag"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>